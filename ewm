import pandas as pd

# Sample DataFrame
data = {
    'ID': ['A', 'A', 'B', 'B', 'C', 'C'],
    'Time': ['2023-01-01', '2023-01-02', '2023-01-01', '2023-01-02', '2023-01-01', '2023-01-02'],
    'Value': [10, 15, 20, 25, 30, 35]
}

df = pd.DataFrame(data)

# Convert the 'Time' column to datetime
df['Time'] = pd.to_datetime(df['Time'])

# Initialize an empty DataFrame to store results
result_df = pd.DataFrame(columns=['ID', 'Time'])

# Iterate over unique IDs
for id in df['ID'].unique():
    # Filter the DataFrame for the current ID
    id_df = df[df['ID'] == id]
    
    # Sort the DataFrame by 'Time' and get the earliest time
    earliest_time = id_df.sort_values('Time')['Time'].iloc[0]
    
    # Append the result to the result DataFrame
    result_df = result_df.append({'ID': id, 'Time': earliest_time}, ignore_index=True)

    # Sort the DataFrame by 'Time' and get the earliest two times
    earliest_two_times = id_df.sort_values('Time')['Time'].head(2).tolist()

    # Append the results to the result DataFrame
    result_df = result_df.append({'ID': id, 'Time': earliest_two_times}, ignore_index=True)

